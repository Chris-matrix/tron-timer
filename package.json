{
  "name": "tron-timer",
  "version": "1.0.0",
  "description": "Tron-themed Focus Timer Application with achievement system and analytics",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "serve": "cross-env NODE_ENV=development node server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "heroku-postbuild": "npm run build",
    "heroku-prebuild": "npm install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "vercel-build": "prisma generate && prisma migrate deploy && vite build",
    "vercel-dev": "prisma generate && prisma migrate dev"
  },
  "keywords": [
    "tron",
    "focus",
    "timer",
    "productivity",
    "achievements",
    "analytics"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^6.9.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.2",
    "framer-motion": "^12.6.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "next-auth": "^4.24.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.43.0",
    "recharts": "^2.15.1",
    "styled-components": "^6.1.16",
    "tsx": "^4.7.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prisma": "^6.9.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0"
  }
}
